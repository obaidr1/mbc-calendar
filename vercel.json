{
  "version": 2,
  "buildCommand": "npx prisma generate && npx prisma db push && npm run build",
  "installCommand": "npm install",
  "builds": [
    {
      "src": ".output/server/index.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": ".output/server/index.mjs"
    }
  ]
} 