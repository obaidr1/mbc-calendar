{
  "name": "mbc-calendar",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.18.0 <19.0.0"
  },
  "scripts": {
    "dev": "prisma generate && nuxt dev",
    "build": "prisma generate && nuxt build",
    "start": "NODE_ENV=production node .output/server/index.mjs",
    "lint": "eslint .",
    "test": "TZ=UTC vitest --run",
    "test:components": "TZ=UTC vitest --run components/",
    "test:watch": "TZ=UTC vitest",
    "test:ui": "TZ=UTC vitest --ui --open",
    "postinstall": "nuxt prepare",
    "generate": "nuxt generate",
    "preview": "DATABASE_URL=file:./db.sqlite nuxt preview",
    "typecheck": "nuxt typecheck",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@huntersofbook/naive-ui-nuxt": "^0.5.1",
    "@nuxt/devtools": "^1.0.8",
    "@nuxt/test-utils": "^3.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/tailwindcss": "^5.3.3",
    "@nuxtjs/vercel-builder": "^0.25.0",
    "@prisma/client": "^5.22.0",
    "@testing-library/vue": "^6.6.1",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.6.1",
    "dayjs": "^1.11.7",
    "eslint": "^8.31.0",
    "h3": "^1.0.2",
    "jsdom": "^21.0.0",
    "nuxt": "^3.8.0",
    "nuxt-svgo": "^1.1.0",
    "playwright": "^1.29.2",
    "prisma": "^5.22.0",
    "typescript": "^4.9.4",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-vue-components": "^0.22.12",
    "uuid": "^9.0.0",
    "vite": "^6.0.0",
    "vite-svg-loader": "^3.6.0",
    "vitest": "^1.6.1",
    "vue-tsc": "^1.0.16",
    "@types/node": "^20.0.0"
  },
  "dependencies": {
    "@types/node": "^20.11.24"
  }
}
